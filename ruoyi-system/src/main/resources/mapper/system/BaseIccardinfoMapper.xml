    </update>

    <delete id="deleteBaseIccardinfoById" parameterType="Long">
        delete from base_iccardinfo where id = #{id}
    </delete>

    <delete id="deleteBaseIccardinfoByIds" parameterType="String">
        delete from base_iccardinfo where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>