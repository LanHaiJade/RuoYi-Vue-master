    <sql id="selectBaseIccardinfoVo">
        select id, machine_id, sb_id, sb_name, user_id, pay_type, cash_type, id_no, cpu_no, amount, return_time, status, update_time, create_time from base_iccardinfo
    </sql>

    <select id="selectBaseIccardinfoList" parameterType="BaseIccardinfo" resultMap="BaseIccardinfoResult">
        <include refid="selectBaseIccardinfoVo"/>
        <where>  
            <if test="machineId != null "> and machine_id = #{machineId}</if>
            <if test="sbId != null  and sbId != ''"> and sb_id = #{sbId}</if>
            <if test="sbName != null  and sbName != ''"> and sb_name like concat('%', #{sbName}, '%')</if>
            <if test="userId != null "> and user_id = #{userId}</if>
            <if test="payType != null "> and pay_type = #{payType}</if>
            <if test="cashType != null "> and cash_type = #{cashType}</if>
            <if test="idNo != null  and idNo != ''"> and id_no = #{idNo}</if>
            <if test="cpuNo != null  and cpuNo != ''"> and cpu_no = #{cpuNo}</if>
            <if test="amount != null "> and amount = #{amount}</if>
            <if test="returnTime != null  and returnTime != ''"> and return_time = #{returnTime}</if>
            <if test="status != null "> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectBaseIccardinfoById" parameterType="Long" resultMap="BaseIccardinfoResult">
        <include refid="selectBaseIccardinfoVo"/>
        where id = #{id}
    </select>

    <insert id="insertBaseIccardinfo" parameterType="BaseIccardinfo" useGeneratedKeys="true" keyProperty="id">
        insert into base_iccardinfo
        <trim prefix="(" suffix=")" suffixOverrides=",">
