            <if test="afterRechargeAmount != null "> and After_recharge_amount = #{afterRechargeAmount}</if>
            <if test="beforeRechargeAmount != null "> and Before_recharge_amount = #{beforeRechargeAmount}</if>
            <if test="rechargeAmount != null "> and recharge_amount = #{rechargeAmount}</if>
