            <if test="ctrlUserId != null "> and ctrl_user_id = #{ctrlUserId}</if>
            <if test="ctrlUserName != null  and ctrlUserName != ''"> and ctrl_user_name like concat('%', #{ctrlUserName}, '%')</if>
            <if test="cpuNo != null  and cpuNo != ''"> and cpu_no = #{cpuNo}</if>
            <if test="giftAmount != null "> and giftAmount = #{giftAmount}</if>
            <if test="beginTime != null and beginTime != ''">
                and update_time &gt;= #{beginTime}
            </if>
            <if test="endTime != null and endTime != ''">
                and update_time &lt;= #{endTime}
            </if>
            <!-- 创建时间模糊查询（字符串匹配） -->
            <if test="updateTime != null and updateTime != ''">
                and DATE_FORMAT(update_time, '%Y-%m-%d %H:%i:%s') like concat('%', #{updateTime}, '%')
            </if>
        </where>
    </select>
    
