<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.BaseSettleAlipayMapper">
    
    <resultMap type="BaseSettleAlipay" id="BaseSettleAlipayResult">
        <result property="id"    column="id"    />
        <result property="tradeNo"    column="trade_no"    />
        <result property="payinfoId"    column="payInfo_id"    />
        <result property="userId"    column="user_id"    />
        <result property="aliConType"    column="ali_con_type"    />
        <result property="amount"    column="amount"    />
        <result property="msg"    column="msg"    />
        <result property="updateTime"    column="update_time"    />
        <result property="createTime"    column="create_time"    />
    </resultMap>

    <sql id="selectBaseSettleAlipayVo">
        select id, trade_no, payInfo_id, user_id, ali_con_type, amount, msg, update_time, create_time from base_settle_alipay
    </sql>

    <select id="selectBaseSettleAlipayList" parameterType="BaseSettleAlipay" resultMap="BaseSettleAlipayResult">
        <include refid="selectBaseSettleAlipayVo"/>
        <where>  
            <if test="tradeNo != null  and tradeNo != ''"> and trade_no = #{tradeNo}</if>
            <if test="payinfoId != null "> and payInfo_id = #{payinfoId}</if>
            <if test="userId != null "> and user_id = #{userId}</if>
            <if test="aliConType != null "> and ali_con_type = #{aliConType}</if>
            <if test="amount != null "> and amount = #{amount}</if>
            <if test="msg != null  and msg != ''"> and msg = #{msg}</if>
        </where>
    </select>
    
    <select id="selectBaseSettleAlipayById" parameterType="Long" resultMap="BaseSettleAlipayResult">
        <include refid="selectBaseSettleAlipayVo"/>
        where id = #{id}
    </select>

    <insert id="insertBaseSettleAlipay" parameterType="BaseSettleAlipay" useGeneratedKeys="true" keyProperty="id">
        insert into base_settle_alipay
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="tradeNo != null">trade_no,</if>
            <if test="payinfoId != null">payInfo_id,</if>
            <if test="userId != null">user_id,</if>
            <if test="aliConType != null">ali_con_type,</if>
            <if test="amount != null">amount,</if>
            <if test="msg != null">msg,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="createTime != null">create_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="tradeNo != null">#{tradeNo},</if>
            <if test="payinfoId != null">#{payinfoId},</if>
            <if test="userId != null">#{userId},</if>
            <if test="aliConType != null">#{aliConType},</if>
            <if test="amount != null">#{amount},</if>
            <if test="msg != null">#{msg},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="createTime != null">#{createTime},</if>
         </trim>
    </insert>

    <update id="updateBaseSettleAlipay" parameterType="BaseSettleAlipay">
        update base_settle_alipay
        <trim prefix="SET" suffixOverrides=",">
            <if test="tradeNo != null">trade_no = #{tradeNo},</if>
            <if test="payinfoId != null">payInfo_id = #{payinfoId},</if>
            <if test="userId != null">user_id = #{userId},</if>
            <if test="aliConType != null">ali_con_type = #{aliConType},</if>
            <if test="amount != null">amount = #{amount},</if>
            <if test="msg != null">msg = #{msg},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteBaseSettleAlipayById" parameterType="Long">
        delete from base_settle_alipay where id = #{id}
    </delete>

    <delete id="deleteBaseSettleAlipayByIds" parameterType="String">
        delete from base_settle_alipay where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>