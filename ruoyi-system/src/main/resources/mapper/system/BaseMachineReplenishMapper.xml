<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.BaseMachineReplenishMapper">
    
    <resultMap type="BaseMachineReplenish" id="BaseMachineReplenishResult">
        <result property="id"    column="id"    />
        <result property="routeId"    column="route_id"    />
        <result property="replenishUserId"    column="replenish_user_id"    />
        <result property="machineId"    column="machine_id"    />
    </resultMap>

    <sql id="selectBaseMachineReplenishVo">
        select id, route_id, replenish_user_id, machine_id from base_machine_replenish
    </sql>

    <select id="selectBaseMachineReplenishList" parameterType="BaseMachineReplenish" resultMap="BaseMachineReplenishResult">
        <include refid="selectBaseMachineReplenishVo"/>
        <where>  
            <if test="routeId != null "> and route_id = #{routeId}</if>
            <if test="replenishUserId != null "> and replenish_user_id = #{replenishUserId}</if>
            <if test="machineId != null "> and machine_id = #{machineId}</if>
        </where>
    </select>
    
    <select id="selectBaseMachineReplenishById" parameterType="Long" resultMap="BaseMachineReplenishResult">
        <include refid="selectBaseMachineReplenishVo"/>
        where id = #{id}
    </select>

    <insert id="insertBaseMachineReplenish" parameterType="BaseMachineReplenish" useGeneratedKeys="true" keyProperty="id">
        insert into base_machine_replenish
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="routeId != null">route_id,</if>
            <if test="replenishUserId != null">replenish_user_id,</if>
            <if test="machineId != null">machine_id,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="routeId != null">#{routeId},</if>
            <if test="replenishUserId != null">#{replenishUserId},</if>
            <if test="machineId != null">#{machineId},</if>
         </trim>
    </insert>

    <update id="updateBaseMachineReplenish" parameterType="BaseMachineReplenish">
        update base_machine_replenish
        <trim prefix="SET" suffixOverrides=",">
            <if test="routeId != null">route_id = #{routeId},</if>
            <if test="replenishUserId != null">replenish_user_id = #{replenishUserId},</if>
            <if test="machineId != null">machine_id = #{machineId},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteBaseMachineReplenishById" parameterType="Long">
        delete from base_machine_replenish where id = #{id}
    </delete>

    <delete id="deleteBaseMachineReplenishByIds" parameterType="String">
        delete from base_machine_replenish where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>