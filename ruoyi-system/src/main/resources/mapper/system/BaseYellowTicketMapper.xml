    <sql id="selectBaseYellowTicketVo">
        select id, order_no, salesman, model, quantity, bulk, client_name, client_phone, client_address, fare, remark, logistic, license, ship, update_time, create_time from base_yellow_ticket
    </sql>

    <select id="selectBaseYellowTicketList" parameterType="BaseYellowTicket" resultMap="BaseYellowTicketResult">
        <include refid="selectBaseYellowTicketVo"/>
        <where>  
            <if test="orderNo != null  and orderNo != ''"> and order_no = #{orderNo}</if>
            <if test="salesman != null  and salesman != ''"> and salesman = #{salesman}</if>
            <if test="model != null  and model != ''"> and model = #{model}</if>
            <if test="quantity != null  and quantity != ''"> and quantity = #{quantity}</if>
            <if test="bulk != null  and bulk != ''"> and bulk = #{bulk}</if>
            <if test="clientName != null  and clientName != ''"> and client_name like concat('%', #{clientName}, '%')</if>
            <if test="clientPhone != null  and clientPhone != ''"> and client_phone = #{clientPhone}</if>
            <if test="clientAddress != null  and clientAddress != ''"> and client_address = #{clientAddress}</if>
            <if test="fare != null  and fare != ''"> and fare = #{fare}</if>
            <if test="logistic != null  and logistic != ''"> and logistic = #{logistic}</if>
            <if test="license != null  and license != ''"> and license = #{license}</if>
            <if test="ship != null  and ship != ''"> and ship = #{ship}</if>
        </where>
    </select>
    
    <select id="selectBaseYellowTicketById" parameterType="Integer" resultMap="BaseYellowTicketResult">
        <include refid="selectBaseYellowTicketVo"/>
        where id = #{id}
    </select>

    <insert id="insertBaseYellowTicket" parameterType="BaseYellowTicket" useGeneratedKeys="true" keyProperty="id">
        insert into base_yellow_ticket
        <trim prefix="(" suffix=")" suffixOverrides=",">
