            <if test="logistic != null">logistic = #{logistic},</if>
            <if test="license != null">license = #{license},</if>
            <if test="ship != null">ship = #{ship},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteBaseYellowTicketById" parameterType="Integer">
        delete from base_yellow_ticket where id = #{id}
    </delete>

    <delete id="deleteBaseYellowTicketByIds" parameterType="String">
        delete from base_yellow_ticket where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
