"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.BaseYellowTicketMapper">
    
    <resultMap type="BaseYellowTicket" id="BaseYellowTicketResult">
        <result property="id"    column="id"    />
        <result property="orderNo"    column="order_no"    />
        <result property="salesman"    column="salesman"    />
        <result property="model"    column="model"    />
        <result property="quantity"    column="quantity"    />
        <result property="bulk"    column="bulk"    />
        <result property="clientName"    column="client_name"    />
        <result property="clientPhone"    column="client_phone"    />
        <result property="clientAddress"    column="client_address"    />
        <result property="fare"    column="fare"    />
        <result property="remark"    column="remark"    />
        <result property="logistic"    column="logistic"    />
        <result property="license"    column="license"    />
