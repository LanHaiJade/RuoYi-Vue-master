            <if test="model != null">model,</if>
            <if test="quantity != null">quantity,</if>
            <if test="bulk != null">bulk,</if>
            <if test="clientName != null">client_name,</if>
            <if test="clientPhone != null">client_phone,</if>
            <if test="clientAddress != null">client_address,</if>
            <if test="fare != null">fare,</if>
            <if test="remark != null">remark,</if>
            <if test="logistic != null">logistic,</if>
            <if test="license != null">license,</if>
            <if test="ship != null">ship,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="createTime != null">create_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="orderNo != null">#{orderNo},</if>
            <if test="salesman != null">#{salesman},</if>
            <if test="model != null">#{model},</if>
            <if test="quantity != null">#{quantity},</if>
            <if test="bulk != null">#{bulk},</if>
            <if test="clientName != null">#{clientName},</if>
            <if test="clientPhone != null">#{clientPhone},</if>
            <if test="clientAddress != null">#{clientAddress},</if>
            <if test="fare != null">#{fare},</if>
            <if test="remark != null">#{remark},</if>
            <if test="logistic != null">#{logistic},</if>
            <if test="license != null">#{license},</if>
            <if test="ship != null">#{ship},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="createTime != null">#{createTime},</if>
         </trim>
    </insert>

    <update id="updateBaseYellowTicket" parameterType="BaseYellowTicket">
        update base_yellow_ticket
        <trim prefix="SET" suffixOverrides=",">
            <if test="orderNo != null">order_no = #{orderNo},</if>
            <if test="salesman != null">salesman = #{salesman},</if>
            <if test="model != null">model = #{model},</if>
            <if test="quantity != null">quantity = #{quantity},</if>
