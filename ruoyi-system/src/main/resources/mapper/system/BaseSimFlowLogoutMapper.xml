<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.BaseSimFlowLogoutMapper">
    
    <resultMap type="BaseSimFlowLogout" id="BaseSimFlowLogoutResult">
        <result property="id"    column="id"    />
        <result property="sbId"    column="sb_id"    />
        <result property="simCard"    column="sim_card"    />
        <result property="createTime"    column="create_time"    />
        <result property="userId"    column="user_id"    />
    </resultMap>

    <sql id="selectBaseSimFlowLogoutVo">
        select id, sb_id, sim_card, create_time, user_id from base_sim_flow_logout
    </sql>

    <select id="selectBaseSimFlowLogoutList" parameterType="BaseSimFlowLogout" resultMap="BaseSimFlowLogoutResult">
        <include refid="selectBaseSimFlowLogoutVo"/>
        <where>  
            <if test="sbId != null  and sbId != ''"> and sb_id = #{sbId}</if>
            <if test="simCard != null  and simCard != ''"> and sim_card = #{simCard}</if>
            <if test="userId != null "> and user_id = #{userId}</if>
        </where>
    </select>
    
    <select id="selectBaseSimFlowLogoutById" parameterType="Long" resultMap="BaseSimFlowLogoutResult">
        <include refid="selectBaseSimFlowLogoutVo"/>
        where id = #{id}
    </select>

    <insert id="insertBaseSimFlowLogout" parameterType="BaseSimFlowLogout" useGeneratedKeys="true" keyProperty="id">
        insert into base_sim_flow_logout
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="sbId != null">sb_id,</if>
            <if test="simCard != null">sim_card,</if>
            <if test="createTime != null">create_time,</if>
            <if test="userId != null">user_id,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="sbId != null">#{sbId},</if>
            <if test="simCard != null">#{simCard},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="userId != null">#{userId},</if>
         </trim>
    </insert>

    <update id="updateBaseSimFlowLogout" parameterType="BaseSimFlowLogout">
        update base_sim_flow_logout
        <trim prefix="SET" suffixOverrides=",">
            <if test="sbId != null">sb_id = #{sbId},</if>
            <if test="simCard != null">sim_card = #{simCard},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="userId != null">user_id = #{userId},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteBaseSimFlowLogoutById" parameterType="Long">
        delete from base_sim_flow_logout where id = #{id}
    </delete>

    <delete id="deleteBaseSimFlowLogoutByIds" parameterType="String">
        delete from base_sim_flow_logout where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>