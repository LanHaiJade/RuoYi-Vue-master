    <sql id="selectBaseTransferAccountVo">
        select id, order_no, month_date, user_id, pay_type, amount, plamount, jtamount, open_id, payee_realname, payee_account, payer_realname, transaction_id, remark, reason, has_notice, payChannelType, sub_mch, status, update_time, create_time, amount_wkf, ratedis, amount_ratedis from base_transfer_account
