<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.BaseIccardPromotionsMapper">
    
    <resultMap type="BaseIccardPromotions" id="BaseIccardPromotionsResult">
        <result property="id"    column="id"    />
        <result property="userId"    column="userId"    />
        <result property="type"    column="type"    />
        <result property="sufficeAmount"    column="suffice_amount"    />
        <result property="giftAmount"    column="gift_amount"    />
        <result property="startTime"    column="start_time"    />
        <result property="endTime"    column="end_time"    />
        <result property="status"    column="status"    />
        <result property="updateTime"    column="update_time"    />
        <result property="createTime"    column="create_time"    />
    </resultMap>

    <sql id="selectBaseIccardPromotionsVo">
        select id, userId, type, suffice_amount, gift_amount, start_time, end_time, status, update_time, create_time from base_iccard_promotions
    </sql>

    <select id="selectBaseIccardPromotionsList" parameterType="BaseIccardPromotions" resultMap="BaseIccardPromotionsResult">
        <include refid="selectBaseIccardPromotionsVo"/>
        <where>  
            <if test="userId != null "> and userId = #{userId}</if>
            <if test="type != null "> and type = #{type}</if>
            <if test="sufficeAmount != null "> and suffice_amount = #{sufficeAmount}</if>
            <if test="giftAmount != null "> and gift_amount = #{giftAmount}</if>
            <if test="startTime != null "> and start_time = #{startTime}</if>
            <if test="endTime != null "> and end_time = #{endTime}</if>
            <if test="status != null "> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectBaseIccardPromotionsById" parameterType="Integer" resultMap="BaseIccardPromotionsResult">
        <include refid="selectBaseIccardPromotionsVo"/>
        where id = #{id}
    </select>

    <insert id="insertBaseIccardPromotions" parameterType="BaseIccardPromotions" useGeneratedKeys="true" keyProperty="id">
        insert into base_iccard_promotions
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="userId != null">userId,</if>
            <if test="type != null">type,</if>
            <if test="sufficeAmount != null">suffice_amount,</if>
            <if test="giftAmount != null">gift_amount,</if>
            <if test="startTime != null">start_time,</if>
            <if test="endTime != null">end_time,</if>
            <if test="status != null">status,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="createTime != null">create_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="userId != null">#{userId},</if>
            <if test="type != null">#{type},</if>
            <if test="sufficeAmount != null">#{sufficeAmount},</if>
            <if test="giftAmount != null">#{giftAmount},</if>
            <if test="startTime != null">#{startTime},</if>
            <if test="endTime != null">#{endTime},</if>
            <if test="status != null">#{status},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="createTime != null">#{createTime},</if>
         </trim>
    </insert>

    <update id="updateBaseIccardPromotions" parameterType="BaseIccardPromotions">
        update base_iccard_promotions
        <trim prefix="SET" suffixOverrides=",">
            <if test="userId != null">userId = #{userId},</if>
            <if test="type != null">type = #{type},</if>
            <if test="sufficeAmount != null">suffice_amount = #{sufficeAmount},</if>
            <if test="giftAmount != null">gift_amount = #{giftAmount},</if>
            <if test="startTime != null">start_time = #{startTime},</if>
            <if test="endTime != null">end_time = #{endTime},</if>
            <if test="status != null">status = #{status},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteBaseIccardPromotionsById" parameterType="Integer">
        delete from base_iccard_promotions where id = #{id}
    </delete>

    <delete id="deleteBaseIccardPromotionsByIds" parameterType="String">
        delete from base_iccard_promotions where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>