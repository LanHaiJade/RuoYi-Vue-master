<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.BaseAreaFpMapper">
    
    <resultMap type="BaseAreaFp" id="BaseAreaFpResult">
        <result property="regionid"    column="regionid"    />
        <result property="province"    column="province"    />
        <result property="provincename"    column="provincename"    />
        <result property="city"    column="city"    />
        <result property="cityname"    column="cityname"    />
        <result property="district"    column="district"    />
        <result property="districtname"    column="districtname"    />
        <result property="lng"    column="lng"    />
        <result property="lat"    column="lat"    />
        <result property="status"    column="status"    />
        <result property="updatetime"    column="updatetime"    />
    </resultMap>

    <sql id="selectBaseAreaFpVo">
        select regionid, province, provincename, city, cityname, district, districtname, lng, lat, status, updatetime from base_area_fp
    </sql>

    <select id="selectBaseAreaFpList" parameterType="BaseAreaFp" resultMap="BaseAreaFpResult">
        <include refid="selectBaseAreaFpVo"/>
        <where>  
            <if test="province != null  and province != ''"> and province = #{province}</if>
            <if test="provincename != null  and provincename != ''"> and provincename like concat('%', #{provincename}, '%')</if>
            <if test="city != null  and city != ''"> and city = #{city}</if>
            <if test="cityname != null  and cityname != ''"> and cityname like concat('%', #{cityname}, '%')</if>
            <if test="district != null  and district != ''"> and district = #{district}</if>
            <if test="districtname != null  and districtname != ''"> and districtname like concat('%', #{districtname}, '%')</if>
            <if test="lng != null  and lng != ''"> and lng = #{lng}</if>
            <if test="lat != null  and lat != ''"> and lat = #{lat}</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="updatetime != null "> and updatetime = #{updatetime}</if>
        </where>
    </select>
    
    <select id="selectBaseAreaFpByRegionid" parameterType="String" resultMap="BaseAreaFpResult">
        <include refid="selectBaseAreaFpVo"/>
        where regionid = #{regionid}
    </select>

    <insert id="insertBaseAreaFp" parameterType="BaseAreaFp">
        insert into base_area_fp
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="regionid != null">regionid,</if>
            <if test="province != null and province != ''">province,</if>
            <if test="provincename != null and provincename != ''">provincename,</if>
            <if test="city != null and city != ''">city,</if>
            <if test="cityname != null and cityname != ''">cityname,</if>
            <if test="district != null and district != ''">district,</if>
            <if test="districtname != null and districtname != ''">districtname,</if>
            <if test="lng != null and lng != ''">lng,</if>
            <if test="lat != null and lat != ''">lat,</if>
            <if test="status != null">status,</if>
            <if test="updatetime != null">updatetime,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="regionid != null">#{regionid},</if>
            <if test="province != null and province != ''">#{province},</if>
            <if test="provincename != null and provincename != ''">#{provincename},</if>
            <if test="city != null and city != ''">#{city},</if>
            <if test="cityname != null and cityname != ''">#{cityname},</if>
            <if test="district != null and district != ''">#{district},</if>
            <if test="districtname != null and districtname != ''">#{districtname},</if>
            <if test="lng != null and lng != ''">#{lng},</if>
            <if test="lat != null and lat != ''">#{lat},</if>
            <if test="status != null">#{status},</if>
            <if test="updatetime != null">#{updatetime},</if>
         </trim>
    </insert>

    <update id="updateBaseAreaFp" parameterType="BaseAreaFp">
        update base_area_fp
        <trim prefix="SET" suffixOverrides=",">
            <if test="province != null and province != ''">province = #{province},</if>
            <if test="provincename != null and provincename != ''">provincename = #{provincename},</if>
            <if test="city != null and city != ''">city = #{city},</if>
            <if test="cityname != null and cityname != ''">cityname = #{cityname},</if>
            <if test="district != null and district != ''">district = #{district},</if>
            <if test="districtname != null and districtname != ''">districtname = #{districtname},</if>
            <if test="lng != null and lng != ''">lng = #{lng},</if>
            <if test="lat != null and lat != ''">lat = #{lat},</if>
            <if test="status != null">status = #{status},</if>
            <if test="updatetime != null">updatetime = #{updatetime},</if>
        </trim>
        where regionid = #{regionid}
    </update>

    <delete id="deleteBaseAreaFpByRegionid" parameterType="String">
        delete from base_area_fp where regionid = #{regionid}
    </delete>

    <delete id="deleteBaseAreaFpByRegionids" parameterType="String">
        delete from base_area_fp where regionid in 
        <foreach item="regionid" collection="array" open="(" separator="," close=")">
            #{regionid}
        </foreach>
    </delete>
</mapper>