<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.BaseIpCheckMapper">
    
    <resultMap type="BaseIpCheck" id="BaseIpCheckResult">
        <result property="id"    column="id"    />
        <result property="ip"    column="ip"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectBaseIpCheckVo">
        select id, ip, remark from base_ip_check
    </sql>

    <select id="selectBaseIpCheckList" parameterType="BaseIpCheck" resultMap="BaseIpCheckResult">
        <include refid="selectBaseIpCheckVo"/>
        <where>  
            <if test="ip != null  and ip != ''"> and ip = #{ip}</if>
        </where>
    </select>
    
    <select id="selectBaseIpCheckById" parameterType="Integer" resultMap="BaseIpCheckResult">
        <include refid="selectBaseIpCheckVo"/>
        where id = #{id}
    </select>

    <insert id="insertBaseIpCheck" parameterType="BaseIpCheck" useGeneratedKeys="true" keyProperty="id">
        insert into base_ip_check
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="ip != null">ip,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="ip != null">#{ip},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateBaseIpCheck" parameterType="BaseIpCheck">
        update base_ip_check
        <trim prefix="SET" suffixOverrides=",">
            <if test="ip != null">ip = #{ip},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteBaseIpCheckById" parameterType="Integer">
        delete from base_ip_check where id = #{id}
    </delete>

    <delete id="deleteBaseIpCheckByIds" parameterType="String">
        delete from base_ip_check where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>