<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.BaseSocketStatisticsMapper">
    
    <resultMap type="BaseSocketStatistics" id="BaseSocketStatisticsResult">
        <result property="id"    column="id"    />
        <result property="sbId"    column="sb_id"    />
        <result property="socketId"    column="socket_id"    />
        <result property="startTime"    column="start_time"    />
        <result property="endTime"    column="end_time"    />
        <result property="createTime"    column="create_time"    />
        <result property="serverIp"    column="server_ip"    />
    </resultMap>

    <sql id="selectBaseSocketStatisticsVo">
        select id, sb_id, socket_id, start_time, end_time, create_time, server_ip from base_socket_statistics
    </sql>

    <select id="selectBaseSocketStatisticsList" parameterType="BaseSocketStatistics" resultMap="BaseSocketStatisticsResult">
        <include refid="selectBaseSocketStatisticsVo"/>
        <where>  
            <if test="sbId != null  and sbId != ''"> and sb_id = #{sbId}</if>
            <if test="socketId != null  and socketId != ''"> and socket_id = #{socketId}</if>
            <if test="startTime != null "> and start_time = #{startTime}</if>
            <if test="endTime != null "> and end_time = #{endTime}</if>
            <if test="serverIp != null  and serverIp != ''"> and server_ip = #{serverIp}</if>
        </where>
    </select>
    
    <select id="selectBaseSocketStatisticsById" parameterType="Long" resultMap="BaseSocketStatisticsResult">
        <include refid="selectBaseSocketStatisticsVo"/>
        where id = #{id}
    </select>

    <insert id="insertBaseSocketStatistics" parameterType="BaseSocketStatistics" useGeneratedKeys="true" keyProperty="id">
        insert into base_socket_statistics
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="sbId != null">sb_id,</if>
            <if test="socketId != null">socket_id,</if>
            <if test="startTime != null">start_time,</if>
            <if test="endTime != null">end_time,</if>
            <if test="createTime != null">create_time,</if>
            <if test="serverIp != null">server_ip,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="sbId != null">#{sbId},</if>
            <if test="socketId != null">#{socketId},</if>
            <if test="startTime != null">#{startTime},</if>
            <if test="endTime != null">#{endTime},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="serverIp != null">#{serverIp},</if>
         </trim>
    </insert>

    <update id="updateBaseSocketStatistics" parameterType="BaseSocketStatistics">
        update base_socket_statistics
        <trim prefix="SET" suffixOverrides=",">
            <if test="sbId != null">sb_id = #{sbId},</if>
            <if test="socketId != null">socket_id = #{socketId},</if>
            <if test="startTime != null">start_time = #{startTime},</if>
            <if test="endTime != null">end_time = #{endTime},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="serverIp != null">server_ip = #{serverIp},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteBaseSocketStatisticsById" parameterType="Long">
        delete from base_socket_statistics where id = #{id}
    </delete>

    <delete id="deleteBaseSocketStatisticsByIds" parameterType="String">
        delete from base_socket_statistics where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>